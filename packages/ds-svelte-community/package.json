{
	"name": "@nais/ds-svelte-community",
	"version": "0.7.5",
	"homepage": "https://nais.github.io/ds-svelte-community",
	"repository": "https://github.com/nais/ds-svelte-community.git",
	"license": "MIT",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build && bun run package",
		"build:docs": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"package": "svelte-package && publint",
		"prepublishOnly": "bun run test:unit && bun run check && bun run lint && bun run package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate-icons": "./hack/generate-icons.sh",
		"test": "bun run test:unit",
		"test:unit": "bun test",
		"test:unit:watch": "bun test --watch"
	},
	"publishConfig": {
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"svelte": "./dist/index.js",
				"default": "./dist/index.js"
			},
			"./icons": {
				"types": "./dist/icons/index.d.ts",
				"import": "./dist/icons/index.js",
				"svelte": "./dist/icons/index.js"
			},
			"./css": {
				"default": "./dist/css/index.css"
			},
			"./*": {
				"types": "./dist/*",
				"import": "./dist/*",
				"svelte": "./dist/*"
			},
			"typesVersions": {
				">4.0": {
					"index.d.ts": [
						"./dist/index.d.ts"
					],
					"icons": [
						"./dist/icons/index.d.ts"
					]
				}
			}
		}
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"import": "./dist/icons/index.js",
			"svelte": "./dist/icons/index.js"
		},
		"./css": {
			"default": "./dist/css/index.css"
		},
		"./*": {
			"types": "./dist/*",
			"import": "./dist/*",
			"svelte": "./dist/*"
		}
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"icons": [
				"./dist/icons/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.59.0 || ^4.0.0-0 || ^5.0.0-next.0 || ^5.0.0",
		"vite": "^4.0.0-0 || ^5.0.0"
	},
	"optionalDependencies": {
		"svelte-floating-ui": "^1.5.9"
	},
	"devDependencies": {
		"@happy-dom/global-registrator": "^14.12.3",
		"@nais/ds-svelte-community-preprocess-svelte": "workspace:*",
		"@navikt/aksel-icons": "^6.13.0",
		"@navikt/ds-react": "^6.13.0",
		"@navikt/ds-tailwind": "^6.13.0",
		"@sveltejs/adapter-auto": "^3.2.2",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.18",
		"@sveltejs/package": "^2.3.2",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"@testing-library/svelte": "^5.2.1",
		"@types/bun": "^1.1.6",
		"@types/diff": "^5.2.1",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.12.0",
		"@typescript-eslint/parser": "^7.12.0",
		"autoprefixer": "^10.4.19",
		"clsx": "^2.1.1",
		"diff": "^5.2.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"file-system-cache": "2.4.4",
		"jsdom": "^24.1.1",
		"mdsvex": "^0.11.2",
		"normalize.css": "^8.0.1",
		"postcss": "^8.4.40",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"publint": "^0.2.9",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"svelte": "^5.0.0-next.201",
		"svelte-check": "^3.8.4",
		"svelte-floating-ui": "^1.5.9",
		"svgo": "3.0.3",
		"tailwindcss": "^3.4.4",
		"totalist": "^3.0.1",
		"tslib": "^2.6.3",
		"typescript": "5.4.5",
		"vite": "5.3.5"
	},
	"dependencies": {
		"@nais/vite-plugin-svelte-docs": "workspace:*",
		"@navikt/ds-css": "^6.13.0",
		"@navikt/ds-tokens": "^6.13.0",
		"shiki": "^1.12.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"overrides": {
		"svelte": "$svelte"
	}
}
