{
	"name": "@nais/ds-svelte-community",
	"version": "1.0.0",
	"homepage": "https://nais.github.io/ds-svelte-community",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nais/ds-svelte-community.git",
		"directory": "packages/ds-svelte-community"
	},
	"license": "MIT",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build && bun run package",
		"build:package": "bun run package",
		"build:docs": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"package": "svelte-package && publint",
		"prepublishOnly": "bun run test:unit && bun run check && bun run lint && bun run package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"generate-icons": "./hack/generate-icons.sh",
		"prepare": "svelte-kit sync || echo ''",
		"test": "bun run test:unit",
		"test:unit": "bun test",
		"test:unit:watch": "bun test --watch",
		"sync": "svelte-kit sync"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"import": "./dist/icons/index.js",
			"svelte": "./dist/icons/index.js"
		},
		"./experimental": {
			"types": "./dist/experimental/index.d.ts",
			"import": "./dist/experimental/index.js",
			"svelte": "./dist/experimental/index.js"
		},
		"./css": {
			"default": "./dist/css/index.css"
		},
		"./*": {
			"types": "./dist/*",
			"import": "./dist/*",
			"svelte": "./dist/*"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^5.20.0"
	},
	"optionalDependencies": {
		"svelte-floating-ui": "^1.6.2"
	},
	"devDependencies": {
		"@nais/vite-plugin-svelte-docs": "workspace:*",
		"@navikt/aksel-icons": "^7.25.1",
		"@navikt/ds-react": "^7.25.1",
		"@navikt/ds-tailwind": "^7.25.1",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.27.0",
		"@sveltejs/package": "^2.4.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.0",
		"@tailwindcss/vite": "^4.1.11",
		"@types/bun": "^1.2.19",
		"@types/diff": "^8.0.0",
		"@types/react-dom": "^19.1.7",
		"@types/selenium-webdriver": "^4.1.28",
		"autoprefixer": "^10.4.21",
		"diff": "^8.0.2",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.11.0",
		"estree-walker": "^3.0.3",
		"global-jsdom": "^26.0.0",
		"jsdom": "^26.1.0",
		"looks-same": "^9.0.1",
		"marked": "^16.1.2",
		"mdsvex": "^0.12.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.12",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"resize-observer-polyfill": "^1.5.1",
		"selenium-webdriver": "^4.34.0",
		"shiki": "^3.9.2",
		"svelte": "5.37.3",
		"svelte-check": "^4.3.1",
		"svelte-floating-ui": "^1.6.2",
		"svgo": "4.0.0",
		"tailwindcss": "^4.1.11",
		"typescript": "5.9.2",
		"typescript-eslint": "^8.38.0",
		"vite": "^7.0.6"
	},
	"dependencies": {
		"@navikt/ds-css": "^7.25.1",
		"@navikt/ds-tokens": "^7.25.1"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"overrides": {
		"svelte": "$svelte"
	}
}
