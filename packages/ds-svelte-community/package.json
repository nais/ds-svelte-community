{
	"name": "@nais/ds-svelte-community",
	"version": "2.0.0-next.7",
	"homepage": "https://nais.github.io/ds-svelte-community",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nais/ds-svelte-community.git",
		"directory": "packages/ds-svelte-community"
	},
	"license": "MIT",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build && bun run package",
		"build:package": "bun run package",
		"build:docs": "bunx --bun vite build",
		"preview": "bunx --bun vite preview",
		"package": "svelte-package && publint",
		"prepublishOnly": "bun run test:unit && bun run check && bun run lint && bun run package",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --log-level warn --check . && eslint . && bun run check-peer-deps",
		"sync-peer-deps": "bun run scripts/sync-peer-deps.ts",
		"check-peer-deps": "bun run scripts/sync-peer-deps.ts --check",
		"format": "prettier --log-level warn --write .",
		"generate-icons": "./hack/generate-icons.sh",
		"prepare": "svelte-kit sync || echo ''",
		"test": "bun run test:unit",
		"test:unit": "bun test",
		"test:unit:watch": "bun test --watch",
		"test:visual-report": "bun run scripts/visual-test-reporter.ts",
		"sync": "svelte-kit sync"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"import": "./dist/icons/index.js",
			"svelte": "./dist/icons/index.js"
		},
		"./experimental": {
			"types": "./dist/experimental/index.d.ts",
			"import": "./dist/experimental/index.js",
			"svelte": "./dist/experimental/index.js"
		},
		"./css": {
			"default": "./dist/css/index.css"
		},
		"./*": {
			"types": "./dist/*",
			"import": "./dist/*",
			"svelte": "./dist/*"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@navikt/ds-css": "8.4.1",
		"@navikt/ds-tokens": "8.4.1",
		"svelte": "^5.20.0"
	},
	"optionalDependencies": {
		"svelte-floating-ui": "^1.6.2"
	},
	"devDependencies": {
		"@navikt/ds-css": "8.4.1",
		"@navikt/ds-tokens": "8.4.1",
		"@nais/vite-plugin-svelte-docs": "workspace:*",
		"@navikt/aksel-icons": "^8.4.1",
		"@navikt/ds-react": "^8.4.1",
		"@navikt/ds-tailwind": "^8.4.1",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.52.0",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"@types/bun": "^1.3.9",
		"@types/diff": "^8.0.0",
		"@types/estree": "^1.0.8",
		"@types/react": "^19.2.14",
		"@types/react-dom": "^19.2.3",
		"@types/selenium-webdriver": "^4.35.5",
		"autoprefixer": "^10.4.24",
		"diff": "^8.0.3",
		"eslint": "^10.0.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"estree-walker": "^3.0.3",
		"global-jsdom": "^28.0.0",
		"jsdom": "^28.1.0",
		"looks-same": "^10.0.1",
		"magic-string": "^0.30.21",
		"marked": "^17.0.2",
		"mdsvex": "^0.12.6",
		"prettier": "^3.8.1",
		"publint": "^0.3.17",
		"react": "^19.2.4",
		"react-dom": "^19.2.4",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-slug": "^6.0.0",
		"resize-observer-polyfill": "^1.5.1",
		"selenium-webdriver": "^4.40.0",
		"shiki": "^3.22.0",
		"svelte": "5.51.2",
		"svelte-check": "^4.4.0",
		"svelte-floating-ui": "^1.6.2",
		"svgo": "4.0.0",
		"tailwindcss": "^4.1.18",
		"typescript": "5.9.3",
		"typescript-eslint": "^8.55.0",
		"vite": "^7.3.1",
		"zimmerframe": "^1.1.4"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module"
}
