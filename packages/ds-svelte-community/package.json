{
	"name": "@nais/ds-svelte-community",
	"version": "0.5.4",
	"homepage": "https://nais.github.io/ds-svelte-community",
	"repository": "https://github.com/nais/ds-svelte-community.git",
	"license": "MIT",
	"scripts": {
		"dev": "bun --bun x vite dev",
		"build": "vite build && bun run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "bun run test:unit && bun run check && bun run lint && bun run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"storybook": "storybook dev -p 6006 --no-open",
		"build:storybook": "storybook build",
		"storybook:docs": "storybook dev --docs",
		"generate-icons": "./hack/generate-icons.sh",
		"test": "svelte-kit sync && bun run test:unit",
		"test:unit": "bun test",
		"test:unit:watch": "bun test --watch"
	},
	"publishConfig": {
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"svelte": "./dist/index.js",
				"default": "./dist/index.js"
			},
			"./icons": {
				"types": "./dist/icons/index.d.ts",
				"import": "./dist/icons/index.js",
				"svelte": "./dist/icons/index.js"
			},
			"./css": {
				"default": "./dist/css/index.css"
			},
			"./*": {
				"types": "./dist/*",
				"import": "./dist/*",
				"svelte": "./dist/*"
			},
			"typesVersions": {
				">4.0": {
					"index.d.ts": [
						"./dist/index.d.ts"
					],
					"icons": [
						"./dist/icons/index.d.ts"
					]
				}
			}
		}
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"import": "./dist/icons/index.js",
			"svelte": "./dist/icons/index.js"
		},
		"./css": {
			"default": "./dist/css/index.css"
		},
		"./*": {
			"types": "./dist/*",
			"import": "./dist/*",
			"svelte": "./dist/*"
		}
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			],
			"icons": [
				"./dist/icons/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.59.0 || ^4.0.0-0"
	},
	"devDependencies": {
		"@happy-dom/global-registrator": "^12.10.1",
		"@nais/ds-svelte-community-preprocess-svelte": "workspace:*",
		"@navikt/aksel-icons": "^5.7.6",
		"@navikt/ds-react": "^5.7.6",
		"@navikt/ds-tailwind": "^5.7.6",
		"@storybook/addon-a11y": "7.5.1",
		"@storybook/addon-essentials": "7.5.1",
		"@storybook/addon-interactions": "7.5.1",
		"@storybook/addon-links": "7.5.1",
		"@storybook/addon-storysource": "7.5.1",
		"@storybook/addon-svelte-csf": "4.0.9",
		"@storybook/blocks": "7.5.1",
		"@storybook/svelte": "7.5.1",
		"@storybook/sveltekit": "7.5.1",
		"@storybook/testing-library": "^0.2.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.27.1",
		"@sveltejs/package": "^2.2.2",
		"@sveltejs/vite-plugin-svelte": "^2.4.6",
		"@testing-library/svelte": "^4.0.4",
		"@types/diff": "^5.0.7",
		"@types/react-dom": "^18.2.14",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"autoprefixer": "^10.4.16",
		"bun-types": "^1.0.7",
		"clsx": "^2.0.0",
		"diff": "^5.1.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-storybook": "^0.6.15",
		"eslint-plugin-svelte": "^2.34.0",
		"file-system-cache": "2.4.4",
		"jsdom": "^22.1.0",
		"normalize.css": "^8.0.1",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"publint": "^0.2.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "7.5.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"svgo": "^3.0.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "5.2.2",
		"vite": "4.5.0"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@navikt/ds-css": "^5.7.6",
		"@navikt/ds-tokens": "^5.7.6",
		"locally-unique-id-generator": "^0.1.5"
	},
	"overrides": {
		"@storybook/cli": {
			"prettier": "$prettier"
		}
	}
}
