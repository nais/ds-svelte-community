{
	"name": "@nais/vite-plugin-svelte-docs",
	"module": "dist/index.js",
	"type": "module",
	"devDependencies": {
		"@types/bun": "1.1.6",
		"@typescript-eslint/eslint-plugin": "^7.17.0",
		"@typescript-eslint/parser": "^7.17.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.43.0",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.0.0-next.195",
		"typedoc": "^0.26.5",
		"typescript": "~5.5.2"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"lint": "eslint ."
	},
	"peerDependencies": {
		"typescript": "~5.2.2"
	},
	"dependencies": {
		"svelte2tsx": "0.7.13",
		"vite": "^5.3.4"
	},
	"publishConfig": {
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"typesVersions": {
				">4.0": {
					"index.d.ts": [
						"./dist/index.d.ts"
					]
				}
			}
		}
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"dist/*",
		"dist/**/*",
		"dist/__snapshots__/**/*",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!dist/build/**/*",
		"src/*.ts",
		"!src/**/*.test.*",
		"!src/build/**/*"
	]
}
