{
	"name": "@nais/vite-plugin-svelte-docs",
	"module": "dist/index.js",
	"type": "module",
	"version": "0.0.2",
	"private": true,
	"devDependencies": {
		"@types/bun": "1.2.17",
		"eslint": "^9.29.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.3",
		"prettier": "^3.6.0",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "5.34.7",
		"tsconfig-resolver": "^3.0.1",
		"typescript": "~5.8.3"
	},
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"check": "tsc",
		"lint": "eslint .",
		"test": "bun test"
	},
	"peerDependencies": {
		"typescript": "^5.8.0",
		"vite": "^6.3.3"
	},
	"dependencies": {
		"svelte2tsx": "0.7.40",
		"vite": "^6.3.5"
	},
	"publishConfig": {
		"types": "./dist/index.d.ts",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"typesVersions": {
				">4.0": {
					"index.d.ts": [
						"./dist/index.d.ts"
					]
				}
			}
		}
	},
	"exports": {
		".": {
			"bun": "./src/index.ts",
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"typesVersions": {
		">4.0": {
			"index.d.ts": [
				"./dist/index.d.ts"
			]
		}
	},
	"files": [
		"dist",
		"dist/*",
		"dist/**/*",
		"dist/__snapshots__/**/*",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"!dist/build/**/*",
		"src/*.ts",
		"!src/**/*.test.*",
		"!src/build/**/*"
	]
}
