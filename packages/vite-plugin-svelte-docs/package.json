{
  "name": "@nais/vite-plugin-svelte-docs",
  "module": "dist/index.js",
  "type": "module",
  "devDependencies": {
    "@types/bun": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^9.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.39.0",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "svelte": "^5.0.0-next.149",
    "typedoc": "^0.25.13",
    "typescript": "~5.4.5"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json"
  },
  "peerDependencies": {
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "svelte2tsx": "0.7.8",
    "vite": "^5.2.11"
  },
  "publishConfig": {
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "typesVersions": {
        ">4.0": {
          "index.d.ts": [
            "./dist/index.d.ts"
          ]
        }
      }
    }
  },
  "exports": {
    ".": {
      "bun": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "typesVersions": {
    ">4.0": {
      "index.d.ts": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "dist/*",
    "dist/**/*",
    "dist/__snapshots__/**/*",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*",
    "!dist/build/**/*",
    "src/*.ts",
    "!src/**/*.test.*",
    "!src/build/**/*"
  ]
}
